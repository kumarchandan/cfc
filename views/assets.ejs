<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/bower_components/bootstrap/dist/css/bootstrap.min.css' />

    <!-- Material Design fonts -->
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/icon?family=Material+Icons">

    <link rel="stylesheet" type="text/css" href="/bower_components/bootstrap-material-design/dist/css/bootstrap-material-design.css">
    <link rel="stylesheet" type="text/css" href="/bower_components/bootstrap-material-design/dist/css/ripples.min.css">
    <link rel="stylesheet" href="/stylesheets/assets.css">
  </head>
  <body>
    <div class="navbar navbar-inverse navbar-static-top">
      <div class="container">
        <a href="#" class="navbar-brand">Hope Foundation</a>
        <button class="navbar-toggle" data-toggle="collapse" data-target=".navHeaderCollapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <div class="collapse navbar-collapse navHeaderCollapse">
          <ul class="nav navbar-nav navbar-right">
              <li class='active'><a href="#">Assets</a>
              <li><a href="allocate">Allocate</a>
              <li><a href="masterdata">Master Data</a>
              <li><a href="#">Logout</a>
          </ul>   
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        
        <div class="col-md-12">
          
          <form class="form-vertical">
            <div class="form-group">
              

              <div class="col-md-4">
                <label for="company" class="col-md-2 control-label">Company</label>
                <select id="company" class="form-control">
                  <option value="null">Select</option>
                  <option>Wipro</option>
                  <option>SAP Labs</option>
                  <option>TCS</option>
                </select>
              </div>
              

              <div class="col-md-4">
                <label for="product" class="col-md-2 control-label">Product</label>
                <select id="product" class="form-control">
                  <option value="null">Select</option>
                  <option>Laptop</option>
                  <option>Desktop</option>
                  <option>Table</option>
                  <option>printer</option>
                </select>
              </div>
              
              
              <div class="col-md-4">
                <label for="center" class="col-md-2 control-label">Center</label>
                <select id="center" class="form-control">
                  <option value="null">Select</option>
                  <option value="HBLR001">Maruthi Seva Nagar</option>
                  <option value="HBLR002">Yelahanka</option>
                  <option value="HBLR0011">KR puram</option>
                  <option value="HBLR0012">MG Road</option>
                  <option value="HBLR0013">BTM Layout</option>
                  <option value="HBLR003">RT Nagar</option>
                  <option value="HBLR0014">Electronic City</option>
                </select>
              </div>
              
            </div>
        </div>
        
        <div class="form-group">
              <div class="col-md-4">
                <label for="year" class="col-md-2 control-label">Year</label>
                <input class="form-control" type="text" id="year">
              </div>
              
              <div class="col-md-6">
                <button id="search" type="submit" class="btn btn-raised btn-success">Search</button>
              </div>
        </div>

      </form>
        
  </div>
    <br>
    <br>
      <div id="data">

        <div class="list-group" id="content">                                                                                                                 

        </div>
      </div>

    </div>
    
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/javascripts/assets.js"></script>
  </body>
</html>